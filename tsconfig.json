  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*",  
    "src/types/express.d.ts" 
  ],
  "exclude": [
    "node_modules"
  ]
}
